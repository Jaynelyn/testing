{% if content.field_style_variant.0['#markup'] in 'style1' %}
    <section class="dt-section bg-color" data-aos="fade-up">
        <div class="services-1">
            <div class="container">
                {% if content.field_tagline.0 or content.field_title.0 %}
                    <div class="home-1-title">
                        {% if content.field_tagline.0 %}
                            <span>{{ content.field_tagline.0 }}</span>
                        {% endif %}
                        <h2>{{ content.field_title.0 }}</h2>
                    </div>
                {% endif %}
                <div class="services-style-1">
                    {{ content.field_services }}
                </div>
                {% if content.field_description.0 or content.field_link.0 %}
                    <div class="more-services">
                        <div class="icon">
                            <svg id="Group_188" data-name="Group 188" xmlns="http://www.w3.org/2000/svg" width="128" height="80" viewBox="0 0 128 80">
                                <rect data-name="Rectangle 121" width="8" height="8" rx="4" transform="translate(120)" fill="#ee8d5a"/>
                                <rect data-name="Rectangle 122" width="8" height="8" rx="4" transform="translate(96)" fill="#ee8d5a"/>
                                <rect data-name="Rectangle 123" width="8" height="8" rx="4" transform="translate(72)" fill="#ee8d5a"/>
                                <rect data-name="Rectangle 124" width="8" height="8" rx="4" transform="translate(48)" fill="#ee8d5a"/>

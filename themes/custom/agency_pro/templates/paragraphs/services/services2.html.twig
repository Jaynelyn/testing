                                <rect data-name="Rectangle 143" width="8" height="8" rx="4" transform="translate(0 72)" fill="#ee8d5a"/>
                            </svg>
                        </div>
                        <p>{{ content.field_description.0 }}</p>
                        {% if content.field_link.0 %}
                            <a href="{{ content.field_link.0['#url'] }}" class="btn btn-primary btn-style-1 btn-style-lg" title="{{ content.field_link.0['#title'] }}">{{ content.field_link.0['#title'] }}</a>
                        {% endif %}
                    </div>
                {% endif %}
            </div>
        </div>
    </section>

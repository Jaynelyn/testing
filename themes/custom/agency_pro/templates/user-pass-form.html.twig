{% set base_url = url('<front>')  %}
<!-- Register Page banner -->
{% if reset_banner_title %}
    <section class="page-banner" style="background-image: url({{ resetpassword_banner_img }});">
        <div class="container dt-section">
            <div class="heading">
                <h1>{{reset_banner_title}}</h1>
            </div>
        </div>
    </section>
    <nav class="breadcrumb-section" aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/">HOME</a></li>
            <li class="icon">
                <svg xmlns="http://www.w3.org/2000/svg" width="39.932" height="37.846" viewBox="0 0 39.932 37.846">
                    <g class="Group_38512" data-name="Group 38512" transform="translate(39.932) rotate(90)">
                        <path id="Icon_ionic-ios-arrow-back" data-name="Icon ionic-ios-arrow-back" d="M16.156,18.918.849,4.609A2.572,2.572,0,0,1,.849.79a3.048,3.048,0,0,1,4.095,0L22.286,17a2.575,2.575,0,0,1,.084,3.729L4.956,37.057a3,3,0,0,1-2.047.789,3,3,0,0,1-2.047-.789,2.572,2.572,0,0,1,0-3.819Z" transform="translate(0 39.932) rotate(-90)" fill="#fff"/>
                        <path id="Icon_ionic-ios-arrow-back-2" data-name="Icon ionic-ios-arrow-back" d="M16.156,18.918.849,4.609A2.572,2.572,0,0,1,.849.79a3.048,3.048,0,0,1,4.095,0L22.286,17a2.575,2.575,0,0,1,.084,3.729L4.956,37.057a3,3,0,0,1-2.047.789,3,3,0,0,1-2.047-.789,2.572,2.572,0,0,1,0-3.819Z" transform="translate(0 23.128) rotate(-90)" fill="#fff"/>
                    </g>
                </svg>
            </li>
            <li class="breadcrumb-item active" aria-current="page">{{ reset_banner_title }}</li>
        </ol>
    </nav>
{% endif %}
<!-- Password Reset Page -->
<section class="forgot-password dt-section" data-aos="zoom-in-down">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-8 col-md-10 col-12">
                <div class="item">
                    <div class="icon">
                        <svg xmlns="http://www.w3.org/2000/svg" width="33" height="36.005" viewBox="0 0 33 36.005">
                            <g id="Icon_feather-unlock" data-name="Icon feather-unlock" transform="translate(-3 -1.488)">
                                <path id="Path_37990" data-name="Path 37990" d="M7.833,16.5H31.167A3.243,3.243,0,0,1,34.5,19.644v11a3.243,3.243,0,0,1-3.333,3.144H7.833A3.243,3.243,0,0,1,4.5,30.646v-11A3.243,3.243,0,0,1,7.833,16.5Z" transform="translate(0 2.202)" fill="none" stroke="#fff" stroke-linecap="round" stroke-linejoin="round" stroke-width="3"/>
                                <path id="Path_37991" data-name="Path 37991" d="M10.5,17.147V10.859c0-4.038,3.387-7.421,7.845-7.827s8.532,2.3,9.424,6.255" transform="translate(0.287)" fill="none" stroke="#fff" stroke-linecap="round" stroke-linejoin="round" stroke-width="3"/>
                            </g>
                        </svg>
                    </div>
                    <div class="forgot-password-form">
                    {# <form> #}
                        <h2>{{ reset_page_title }}</h2>
                        <div class="form-group">
                            {{ form.name }}
                            {{ form.mail }}
                        </div>
                        <div class="forgot-password-btn">
                            {{ form.actions }}
                            {{ form.form_build_id }}
                            {{ form.form_id }}
                        </div>
                    {# </form> #}
                </div>
            </div>
        </div>
    </div>           
</section>
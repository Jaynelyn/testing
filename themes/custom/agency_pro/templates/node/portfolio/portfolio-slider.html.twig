<section class="portfolio-details dt-section" data-aos="fade-up">
    <div class="container">
        <div class="portfolio-details-slider" data-aos="fade-up">
            <div class="portfolio-slider-2 owl-carousel owl-theme">
                {% for count, item in node.field_slider_images.value %}
                    <div class="image">
                        <img src="{{ file_url(content.field_slider_images[count]['#item'].entity.uri.value)}}"
                            alt="{{ content.field_slider_images[count]['#item'].alt }}"
                            title="{{ content.field_slider_images[count]['#item'].title }}" class="img-fluid">
                    </div>
                {% endfor %}
            </div>
            <div class="arrows">
                <button class="next" title="Title">
                    <svg xmlns="http://www.w3.org/2000/svg" width="19.6" height="35" viewBox="0 0 19.6 35">
                        <path id="Icon_ionic-ios-arrow-forward" data-name="Icon ionic-ios-arrow-forward" d="M17.155,23.69,30.127,10.456a2.528,2.528,0,0,0,0-3.533,2.434,2.434,0,0,0-3.47,0l-14.7,15a2.535,2.535,0,0,0-.071,3.449l14.758,15.1a2.428,2.428,0,0,0,3.47,0,2.528,2.528,0,0,0,0-3.533Z" transform="translate(-11.246 -6.196)" fill="#111"/>
                    </svg>
                </button>
                <button class="prev" title="Title">
                    <svg xmlns="http://www.w3.org/2000/svg" width="19.6" height="35" viewBox="0 0 19.6 35">
                        <path id="Icon_ionic-ios-arrow-forward" data-name="Icon ionic-ios-arrow-forward" d="M24.938,23.69,11.966,10.456a2.528,2.528,0,0,1,0-3.533,2.434,2.434,0,0,1,3.47,0l14.7,15a2.535,2.535,0,0,1,.071,3.449l-14.758,15.1a2.428,2.428,0,0,1-3.47,0,2.528,2.528,0,0,1,0-3.533Z" transform="translate(-11.246 -6.196)" fill="#111"/>
                    </svg>
                </button>
            </div>
        </div>
        <div class="content-and-sidebar" data-aos="fade-up">
            <div class="row">
                <div class="col-lg-8 col-md-7 col-12">
                    <div class="content">
                        <div class="overview">
                            {{ content.field_descriptions.0 }}
                        </div>
                        <div class="challenge-solutions">
                            {{ content.field_sections}}
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 col-md-5 col-12">
                    <div class="project-information-sidebar">
                        <h4>{{ content.field_title_field.0 }}</h4>
                        <ul class="project-information">
                            <li>
                                <div class="icon">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="30.502" height="32.512" viewBox="0 0 30.502 32.512">
                                        <g id="noun_person_3747970" transform="translate(0.252 0.25)">
                                            <path id="Path_2532" data-name="Path 2532" d="M25.88,23.731a9.152,9.152,0,0,0,5.9-10.249,9.473,9.473,0,0,0-18.539,0,9.152,9.152,0,0,0,5.9,10.249A14.878,14.878,0,0,0,7.519,36.881a.956.956,0,0,0,.4.9,1.013,1.013,0,0,0,1,.09.967.967,0,0,0,.567-.812h0a12.924,12.924,0,0,1,13.02-11.775,12.924,12.924,0,0,1,13.02,11.775.982.982,0,0,0,.983.887H36.6a.992.992,0,0,0,.678-.333.942.942,0,0,0,.232-.7A14.877,14.877,0,0,0,25.88,23.731ZM15.071,15.156a7.445,7.445,0,0,1,14.885.012,7.445,7.445,0,0,1-14.885-.012Z" transform="translate(-7.512 -5.963)" fill="#e53637" stroke="#e53637" stroke-width="0.5"/>
                                        </g>
                                    </svg>
                                </div>
                                <div class="info">
                                    <h6>Client</h6>
                                    <span>{{ content.field_client.0 }}</span>
                                </div>
                            </li>
                            <li>
                                <div class="icon">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="22.857" height="32" viewBox="0 0 22.857 32">
                                        <path id="path7" d="M6.65,32.344l6.674-5.362a1.926,1.926,0,0,1,2.206,0l6.679,5.362A2.654,2.654,0,0,0,23.855,33a1.844,1.844,0,0,0,.83-.2,2.431,2.431,0,0,0,1.173-2.3V4.54A3.49,3.49,0,0,0,22.429,1h-16A3.49,3.49,0,0,0,3,4.54V30.5a2.43,2.43,0,0,0,1.17,2.3A2.283,2.283,0,0,0,6.65,32.344ZM5.286,4.54A1.162,1.162,0,0,1,6.429,3.36h16a1.164,1.164,0,0,1,1.143,1.18V30.449l-6.638-5.331a4.139,4.139,0,0,0-5.01,0L5.286,30.449Z" transform="translate(-3 -1)" fill="#e53637"/>
                                    </svg>
                                </div>
                                <div class="info">
                                    <h6>Category</h6>
                                    <span>{{ content.field_tabs.0 }}</span>
                                </div>
                            </li>
                            <li>
                                <div class="icon">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="28.801" height="32" viewBox="0 0 28.801 32">
                                        <path data-name="Path 2538" d="M28.6,5.2H27V3.6a1.6,1.6,0,0,0-3.2,0V5.2H11V3.6a1.6,1.6,0,1,0-3.2,0V5.2H6.2A3.185,3.185,0,0,0,3.016,8.4L3,30.8A3.2,3.2,0,0,0,6.2,34H28.6a3.209,3.209,0,0,0,3.2-3.2V8.4A3.209,3.209,0,0,0,28.6,5.2Zm0,24A1.6,1.6,0,0,1,27,30.8H7.8a1.6,1.6,0,0,1-1.6-1.6v-16H28.6ZM9.4,16.4h3.2v3.2H9.4Zm6.4,0H19v3.2H15.8Zm6.4,0h3.2v3.2H22.2Z" transform="translate(-3 -2)" fill="#e53637"/>
                                    </svg>
                                </div>
                                <div class="info">
                                    <h6>Start Date</h6>
                                    <span>{{ start_date }}</span>
                                </div>
                            </li>
                            <li>
                                <div class="icon">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="28.801" height="32" viewBox="0 0 28.801 32">
